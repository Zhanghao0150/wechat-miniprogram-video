
page{
	background-color: black;
}
.w-1028-wrap{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.tools-bar{
	position: fixed;
	bottom: 90rpx;
	left: 80rpx;
	right: 80rpx;	
	display: -webkit-box;	
	display: -webkit-flex;	
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	z-index: 999;
}
.ret{
	width: 60rpx;
	height: 60rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size: 100% 100%;
}
.share{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	color: white;
	font-size: 40rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	/* border: 1px white solid; */
	border-radius: 50rpx;
	padding: 0 30rpx;
	background-color: rgb(65,168,99);
	z-index: 999;
}
.share-img{
	/* padding: 20rpx 20rpx; */
	/* margin: 20rpx 20rpx; */
	width: 40rpx;
	height: 40rpx;
	background:url(data:image/png;base64,) no-repeat;
	background-size: cover;
}
.swiper-item{
	position: relative;
	z-index: -1;
	width: 100%;
	height: 100vh;
}
.swiper-wrap{
	width: 100%;
	height: 100vh;
}
image, video{
	position: absolute;
	width: 100%;
	height: 100vh;
}
.share-btn {
	border: none; 
	background-color: transparent;
	outline: none;
}
.share-btn::after {
  border: none;
}


